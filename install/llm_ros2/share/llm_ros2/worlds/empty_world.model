  <model name="grabbable_cylinder">

    <plugin name="state_publisher" filename="libgazebo_ros_state_publisher.so">
      <ros>
        <!-- ROS 2话题名称，Gazebo将在这个话题上发布模型的状态 -->
        
        <argument>/position</argument>
      </ros>
    </plugin>

    <pose>-0.55 -0.55 0.40 0 0 0</pose> <!-- 位置调整以防止与地面相交 -->
    <static>false</static> <!-- 非静态，可移动 -->
    <link name="cylinder_link">
      <inertial>
        <mass>0.01</mass> <!-- 质量 -->
        <inertia> <!-- 根据圆柱体的质量和尺寸调整惯性矩阵 -->
          <ixx>0.00000078125</ixx> <!-- ixx -->
          <ixy>0.0</ixy> <!-- ixy -->
          <ixz>0.0</ixz> <!-- ixz -->
          <iyy>0.00000078125</iyy> <!-- iyy -->
          <iyz>0.0</iyz> <!-- iyz -->
          <izz>0.00000078125</izz> <!-- izz -->
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.025</radius> <!-- 半径为0.025米 -->
            <length>0.1</length> <!-- 高度为0.1米 -->
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.025</radius>
            <length>0.1</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.1 0.1 0.8 1</ambient> <!-- 蓝色 -->
          <diffuse>0.1 0.1 0.8 1</diffuse>
        </material>
      </visual>
    </link>
  </model>



    <model name="my_box">
      <static>false</static>
      <pose>1 1 0.012 0 0 0</pose> <!-- 长方体的位置和朝向 -->
      <link name="link">
        <inertial>
          <mass>0.01</mass>
          <inertia>
            <ixx>2.167e-6</ixx>
            <iyy>8.417e-6</iyy>
            <izz>1.042e-5</izz>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyz>0</iyz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.1 0.05 0.024</size> <!-- 长、宽、高 -->
            </box>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>2.0</mu> <!-- 静摩擦系数 -->
                <mu2>1.5</mu2> <!-- 动摩擦系数 -->
              </ode>
            </friction>


            <!-- 可以选择性地添加更多表面属性，如弹性等 -->
          </surface>
         
         

        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.1 0.05 0.03</size> <!-- 长、宽、高 -->
            </box>
          </geometry>
          <!-- 可以定义材质来改变颜色或纹理 -->
          <material>
            <ambient>0.5 0.5 0.5 1</ambient> <!-- RGBA颜色 -->
          </material>
        </visual>
      </link>
    </model>

